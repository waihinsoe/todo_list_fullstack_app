generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id    Int     @id @default(autoincrement())
  name  String
  email String
  image String
  todos todos[]
}

model todos {
  id       Int      @id @default(autoincrement())
  title    String
  note     String
  date     DateTime
  users_id Int
  users    users    @relation(fields: [users_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
